{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.content.draftField": {
    "name": "draft",
    "type": "boolean"
  },
  "frontMatter.content.pageFolders": [
    {
      "filePrefix": null,
      "path": "[[workspace]]/content/posts",
      "previewPath": "/posts/",
      "title": "posts"
    },
    {
      "contentTypes": [
        "md"
      ],
      "path": "[[workspace]]/content/radar",
      "previewPath": "/radar/",
      "title": "Blips"
    },
    {
      "excludeSubdir": true,
      "path": "[[workspace]]/content",
      "title": "Pages"
    }
  ],
  "frontMatter.content.publicFolder": "static",
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.framework.id": "hugo",
  "frontMatter.panel.actions.disabled": [
    "startStopServer",
    "preview",
    "optimizeSlug"
  ],
  "frontMatter.preview.host": "http://localhost:8888",
  "frontMatter.sponsors.ai.enabled": false,
  "frontMatter.taxonomy.contentTypes": [
    {
      "fields": [
        {
          "name": "title",
          "title": "Title",
          "type": "string"
        },
        {
          "name": "description",
          "title": "Description",
          "type": "string"
        },
        {
          "default": "{{now}}",
          "isPublishDate": true,
          "name": "date",
          "title": "Date",
          "type": "datetime"
        },
        {
          "name": "preview",
          "title": "Content preview",
          "type": "image"
        },
        {
          "name": "draft",
          "title": "Is in draft",
          "type": "draft"
        },
        {
          "name": "tags",
          "title": "Tags",
          "type": "tags"
        },
        {
          "name": "categories",
          "title": "Categories",
          "type": "categories"
        },
        {
          "fields": [
            {
              "fields": [
                {
                  "name": "name",
                  "title": "name",
                  "type": "string"
                },
                {
                  "name": "src",
                  "title": "src",
                  "type": "string"
                },
                {
                  "name": "title",
                  "title": "title",
                  "type": "string"
                },
                {
                  "fields": [
                    {
                      "name": "copyright",
                      "title": "copyright",
                      "type": "string"
                    },
                    {
                      "name": "background",
                      "title": "background",
                      "type": "datetime"
                    }
                  ],
                  "name": "params",
                  "title": "params",
                  "type": "fields"
                }
              ],
              "name": "0",
              "title": "0",
              "type": "fields"
            },
            {
              "fields": [],
              "name": "move",
              "title": "move",
              "type": "fields"
            }
          ],
          "name": "resources",
          "title": "resources",
          "type": "block"
        },
        {
          "isModifiedDate": true,
          "name": "lastmod",
          "title": "lastmod",
          "type": "datetime"
        }
      ],
      "name": "default",
      "pageBundle": true,
      "previewPath": null
    }
  ],
  "frontMatter.taxonomy.dateFormat": "yyyy-MM-dd'T'HH:mm:ssxxx",
  "frontMatter.taxonomy.frontMatterType": "YAML",
  "frontMatter.taxonomy.indentArrays": false,
  "frontMatter.templates.folder": "archetypes",
  "frontMatter.website.host": "https://artero.dev"
}

{
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "other": "on",
      "strings": "on"
    }
  },
  "cSpell.words": [
    "changefreq",
    "errorf",
    "goldmark",
    "htmlproofer",
    "lastmod",
    "linkify",
    "loveit",
    "metaindex",
    "metamethod",
    "metamethods",
    "metatable",
    "Metatables",
    "metavalue",
    "monokai",
    "newindex",
    "nolint",
    "preinstall",
    "pygments",
    "segmentit",
    "userdata",
    "warnf"
  ],
  "github-actions.workflows.pinned.workflows": [
    ".github/workflows/pages.yml"
  ],
  "html.format.templating": true,
  "grammarly.selectors": [
    {
      "language": "markdown",
      "scheme": "file"
    }
  ],
  "grammarly.files.include": [
    "**/readme.md",
    "**/README.md",
    "content/**/*.md"
  ]
}
{
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "other": "on",
      "strings": "on"
    }
  },
  "cSpell.words": [
    "airsoft",
    "alertmanager",
    "bookalike",
    "changefreq",
    "emojify",
    "errorf",
    "frontmatter",
    "fsys",
    "githubv",
    "gocolly",
    "gohugoio",
    "goldmark",
    "goodreads",
    "graphviz",
    "gregjones",
    "htmlproofer",
    "httpcache",
    "jsonify",
    "lastmod",
    "leveldbcache",
    "linkify",
    "loveit",
    "metaindex",
    "metamethod",
    "metamethods",
    "metatable",
    "Metatables",
    "metavalue",
    "monokai",
    "newindex",
    "nolint",
    "openlibrary",
    "pageparser",
    "preinstall",
    "pygments",
    "recents",
    "rwfs",
    "segmentit",
    "shurcoo",
    "stefanzweifel",
    "Thoughtworks",
    "Unikernel",
    "userdata",
    "warnf",
    "whitespaces",
    "xlink"
  ],
  "css.format.enable": false,
  "github-actions.workflows.pinned.workflows": [
    ".github/workflows/pages.yml"
  ],
  "grammarly.files.include": [
    "**/readme.md",
    "**/README.md",
    "content/**/*.md"
  ],
  "grammarly.selectors": [
    {
      "language": "markdown",
      "scheme": "file"
    }
  ],
  "html.format.templating": true,
  "less.format.enable": false,
  "scss.format.enable": false,
  "search.exclude": {
    "**/public": true,
    "**/tmp": true
  },
  "files.associations": {
    "**/_headers": "plaintext"
  },
  "webPreview.url": "http://localhost:8888",
  "webPreview.mediaScreenOverride": true,
  "webPreview.mediaScreen": {
    "SM": 640,
    "MD": 768,
    "LG": 1024,
    "XL": 1280,
    "2XL": 1536
  }
}
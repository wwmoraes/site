{
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "other": "on",
      "strings": "on"
    }
  },
  "cSpell.words": [
    "airsoft",
    "alertmanager",
    "allconfig",
    "Audiobook",
    "bookalike",
    "changefreq",
    "dupword",
    "emojify",
    "errorf",
    "frontmatter",
    "fsys",
    "githubv",
    "gocolly",
    "gohugoio",
    "goldmark",
    "gomnd",
    "goodreads",
    "graphviz",
    "gregjones",
    "htmlproofer",
    "httpcache",
    "hugofs",
    "jsonify",
    "lastmod",
    "leveldbcache",
    "librarything",
    "linkify",
    "loveit",
    "metaindex",
    "metamethod",
    "metamethods",
    "metatable",
    "Metatables",
    "metavalue",
    "monokai",
    "newindex",
    "nolint",
    "Ocaid",
    "openlibrary",
    "pageparser",
    "preinstall",
    "pygments",
    "recents",
    "rwfs",
    "segmentit",
    "shurcoo",
    "stefanzweifel",
    "Thoughtworks",
    "tmpl",
    "Unikernel",
    "usecases",
    "userdata",
    "warnf",
    "whitespaces",
    "xlink"
  ],
  "css.format.enable": false,
  "files.associations": {
    "**/_headers": "plaintext"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hugo_build.lock": true
  },
  "github-actions.workflows.pinned.workflows": [
    ".github/workflows/deployment.yaml",
    ".github/workflows/update.yml"
  ],
  "grammarly.files.include": [
    "**/readme.md",
    "**/README.md",
    "content/**/*.md"
  ],
  "grammarly.selectors": [
    {
      "language": "markdown",
      "scheme": "file"
    }
  ],
  "html.format.templating": true,
  "less.format.enable": false,
  "scss.format.enable": false,
  "search.exclude": {
    "**/public": true,
    "**/tmp": true
  },
  "webPreview.mediaScreen": {
    "2XL": 1536,
    "LG": 1024,
    "MD": 768,
    "SM": 640,
    "XL": 1280
  },
  "webPreview.mediaScreenOverride": true,
  "webPreview.url": "http://localhost:8888"
}